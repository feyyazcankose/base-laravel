{
    "openapi": "3.0.0",
    "info": {
        "title": "İşim Sağlık",
        "version": "1.0.0"
    },
    "paths": {
        "/api/admin/login": {
            "post": {
                "tags": [
                    "Dashboard > Auth"
                ],
                "summary": "User login",
                "operationId": "02064f557af8baf1c24fd21c09b689f3",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "example": "admin@example.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "password"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Login successful"
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                }
            }
        },
        "/api/admin/logout": {
            "post": {
                "tags": [
                    "Dashboard > Auth"
                ],
                "summary": "User logout",
                "operationId": "18687458536b64e1e1c3725706529bb2",
                "responses": {
                    "200": {
                        "description": "Successfully logged out"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/admin/current": {
            "get": {
                "tags": [
                    "Dashboard > Auth"
                ],
                "summary": "Get authenticated user details",
                "operationId": "ffcb7e255867aeb48257ebdb0dc04358",
                "responses": {
                    "200": {
                        "description": "User details",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "id": {
                                            "type": "integer",
                                            "example": 1
                                        },
                                        "name": {
                                            "type": "string",
                                            "example": "John Doe"
                                        },
                                        "email": {
                                            "type": "string",
                                            "example": "johndoe@example.com"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/create-role": {
            "post": {
                "tags": [
                    "Dashboard > Role Management"
                ],
                "summary": "Create a new role",
                "operationId": "cea1224bfc44e7c26eba25956c64ef23",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "admin"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Role created successfully"
                    },
                    "400": {
                        "description": "Bad Request"
                    }
                }
            }
        },
        "/api/create-permission": {
            "post": {
                "tags": [
                    "Dashboard > Role Management"
                ],
                "summary": "Create a new permission",
                "operationId": "66399f796c99cc18284eaf223977cdbb",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "edit articles"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Permission created successfully"
                    },
                    "400": {
                        "description": "Bad Request"
                    }
                }
            }
        },
        "/api/assign-role": {
            "post": {
                "tags": [
                    "Dashboard > Role Management"
                ],
                "summary": "Assign a role to a user",
                "operationId": "7602e6eae16eb270e3cbf2c96b733b74",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "user_id",
                                    "role"
                                ],
                                "properties": {
                                    "user_id": {
                                        "type": "integer",
                                        "example": 1
                                    },
                                    "role": {
                                        "type": "string",
                                        "example": "admin"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Role assigned successfully"
                    },
                    "400": {
                        "description": "Bad Request"
                    }
                }
            }
        },
        "/api/assign-permission": {
            "post": {
                "tags": [
                    "Dashboard > Role Management"
                ],
                "summary": "Assign a permission to a role",
                "operationId": "ff16ee07aff14393c79eafb9a27d81c6",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "role",
                                    "permission"
                                ],
                                "properties": {
                                    "role": {
                                        "type": "string",
                                        "example": "admin"
                                    },
                                    "permission": {
                                        "type": "string",
                                        "example": "edit articles"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Permission assigned successfully"
                    },
                    "400": {
                        "description": "Bad Request"
                    }
                }
            }
        },
        "/api/admin/users": {
            "get": {
                "tags": [
                    "Dashboard > User"
                ],
                "summary": "Get list of users",
                "operationId": "286702e08359cfdc69f757c6c1534c17",
                "parameters": [
                    {
                        "$ref": "#/components/parameters/skip"
                    },
                    {
                        "$ref": "#/components/parameters/take"
                    },
                    {
                        "$ref": "#/components/parameters/sort"
                    },
                    {
                        "$ref": "#/components/parameters/group"
                    },
                    {
                        "$ref": "#/components/parameters/filter"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successfully logged out"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        }
    },
    "components": {
        "schemas": {
            "AdminDto": {
                "title": "Admin",
                "required": [
                    "id",
                    "first_name",
                    "last_name",
                    "email",
                    "phone",
                    "phone_code",
                    "account_status",
                    "created_at"
                ],
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": "svm2yb3aeq"
                    },
                    "first_name": {
                        "type": "string",
                        "example": "feyyaz"
                    },
                    "last_name": {
                        "type": "string",
                        "example": "Admin"
                    },
                    "image": {
                        "type": "string",
                        "example": "feyyaz"
                    },
                    "email": {
                        "type": "string",
                        "example": "dev@feyyaz.com.tr"
                    },
                    "phone": {
                        "type": "string",
                        "example": "905429322605"
                    },
                    "phone_code": {
                        "type": "string",
                        "example": "+90"
                    },
                    "account_status": {
                        "type": "boolean",
                        "example": true
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time"
                    }
                },
                "type": "object"
            },
            "FilterGroupDto": {
                "title": "Filter Group",
                "properties": {
                    "data": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/FilterGroupItemDto"
                        }
                    },
                    "totalCount": {
                        "type": "integer",
                        "example": 100
                    }
                },
                "type": "object"
            },
            "FilterGroupItemDto": {
                "title": "Filter Group Item",
                "properties": {
                    "key": {
                        "type": "string",
                        "example": "Konutkonfor"
                    }
                },
                "type": "object"
            },
            "FilterMetaDto": {
                "title": "Filter Meta",
                "properties": {
                    "totalItems": {
                        "type": "integer",
                        "example": 100
                    },
                    "itemCount": {
                        "type": "integer",
                        "example": 10
                    },
                    "itemsPerPage": {
                        "type": "integer",
                        "example": 10
                    },
                    "totalPages": {
                        "type": "integer",
                        "example": 10
                    },
                    "currentPage": {
                        "type": "integer",
                        "example": 1
                    }
                },
                "type": "object"
            }
        },
        "parameters": {
            "skip": {
                "name": "skip",
                "in": "query",
                "required": false,
                "schema": {
                    "type": "integer",
                    "example": 1
                }
            },
            "take": {
                "name": "take",
                "in": "query",
                "required": false,
                "schema": {
                    "type": "integer",
                    "example": 10
                }
            },
            "sort": {
                "name": "sort",
                "in": "query",
                "required": false,
                "schema": {
                    "type": "string",
                    "example": "yourSelector,asc"
                }
            },
            "filter": {
                "name": "filter",
                "in": "query",
                "description": "Array string",
                "required": false,
                "schema": {
                    "type": "string"
                }
            },
            "group": {
                "name": "group",
                "in": "query",
                "description": "Array object",
                "required": false,
                "schema": {
                    "type": "string"
                }
            }
        },
        "securitySchemes": {
            "bearerAuth": {
                "type": "apiKey",
                "name": "Authorization",
                "in": "header",
                "description": "Enter token in format (Bearer <token>)"
            }
        }
    },
    "security": [
        {
            "Bearer": []
        }
    ]
}