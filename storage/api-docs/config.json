                }
            },
            "filter": {
                "name": "filter",
                "in": "query",
                "description": "Array string",
                "required": false,
                "schema": {
                    "type": "string"
                }
            },
            "group": {
                "name": "group",
                "in": "query",
                "description": "Array object",
                "required": false,
                "schema": {
                    "type": "string"
                }
            }
        },
        "securitySchemes": {
            "bearerAuth": {
                "type": "apiKey",
                "name": "Authorization",
                "in": "header",
