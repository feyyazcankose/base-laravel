    "paths": {
        "/api/admins": {
            "get": {
                "summary": "Get paginated list of admins",
                "operationId": "690793f4c53e2c3994be7c9a60812521",
                "responses": {
                    "200": {
                        "description": "Successful responses"
                    }
                }
            }
        },
        "/api/users": {
            "get": {
                "summary": "Get paginated list of admins",
                "operationId": "1fbdca4d106c3f04b94f85640630fa07",
                "responses": {
                    "200": {
                        "description": "Successful responses"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/login": {
            "post": {
                "tags": [
                    "Dashboard > Auth"
                ],
                "summary": "User login",
                "operationId": "ae57828047478c9006ddf44336313723",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "example": "admin@example.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "password"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Login successful"
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                }
            }
        },
        "/api/logout": {
            "post": {
                "tags": [
                    "Dashboard > Auth"
                ],
                "summary": "User logout",
                "operationId": "d66ecc0b487d2fe47fedc84cca5df9cc",
                "responses": {
                    "200": {
                        "description": "Successfully logged out"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/me": {
            "get": {
                "tags": [
                    "Dashboard > Auth"
                ],
                "summary": "Get authenticated user details",
                "operationId": "96b0290df420ceeb887f1fe3561b1137",
                "responses": {
                    "200": {
                        "description": "User details",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "id": {
                                            "type": "integer",
                                            "example": 1
                                        },
                                        "name": {
                                            "type": "string",
                                            "example": "John Doe"
                                        },
                                        "email": {
                                            "type": "string",
                                            "example": "johndoe@example.com"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": "bearer"
            }
        },
        "/api/create-role": {
            "post": {
                "tags": [
                    "Dashboard > Role Management"
                ],
                "summary": "Create a new role",
                "operationId": "95d467a5e214e4ccabf2de85ef5b4556",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "admin"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Role created successfully"
                    },
                    "400": {
                        "description": "Bad Request"
                    }
                }
            }
        },
        "/api/create-permission": {
            "post": {
                "tags": [
                    "Dashboard > Role Management"
                ],
                "summary": "Create a new permission",
                "operationId": "5b5e48d56019e83b0a7641618a927dad",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "edit articles"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Permission created successfully"
                    },
                    "400": {
                        "description": "Bad Request"
                    }
                }
            }
        },
        "/api/assign-role": {
            "post": {
                "tags": [
                    "Dashboard > Role Management"
                ],
                "summary": "Assign a role to a user",
                "operationId": "e68aa1bfeb02f45960f9344a517dafbb",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "user_id",
                                    "role"
                                ],
                                "properties": {
                                    "user_id": {
                                        "type": "integer",
                                        "example": 1
                                    },
                                    "role": {
                                        "type": "string",
                                        "example": "admin"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Role assigned successfully"
                    },
                    "400": {
                        "description": "Bad Request"
                    }
                }
            }
        },
        "/api/assign-permission": {
            "post": {
                "tags": [
                    "Dashboard > Role Management"
                ],
                "summary": "Assign a permission to a role",
                "operationId": "6547154bc818ff37c8aa66885a5c3b75",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "role",
                                    "permission"
                                ],
                                "properties": {
                                    "role": {
                                        "type": "string",
                                        "example": "admin"
                                    },
                                    "permission": {
                                        "type": "string",
                                        "example": "edit articles"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Permission assigned successfully"
                    },
                    "400": {
                        "description": "Bad Request"
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "FilterGroupDto": {
                "title": "Filter Group",
                "properties": {
                    "data": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/FilterGroupItemDto"
                        }
                    },
                    "totalCount": {
                        "type": "integer",
                        "example": 100
                    }
                },
                "type": "object"
            },
            "FilterGroupItemDto": {
                "title": "Filter Group Item",
                "properties": {
                    "key": {
                        "type": "string",
                        "example": "Konutkonfor"
                    }
                },
                "type": "object"
            },
            "FilterMetaDto": {
                "title": "Filter Meta",
                "properties": {
                    "totalItems": {
                        "type": "integer",
                        "example": 100
                    },
                    "itemCount": {
                        "type": "integer",
                        "example": 10
                    },
                    "itemsPerPage": {
                        "type": "integer",
                        "example": 10
                    },
                    "totalPages": {
                        "type": "integer",
                        "example": 10
                    },
                    "currentPage": {
                        "type": "integer",
                        "example": 1
                    }
                },
                "type": "object"
            },
            "AdminDto": {
                "title": "Admin",
                "required": [
                    "id",
                    "first_name",
                    "last_name",
                    "email",
                    "phone",
                    "phone_code",
                    "account_status",
                    "created_at"
                ],
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": "svm2yb3aeq"
                    },
                    "first_name": {
                        "type": "string",
                        "example": "feyyaz"
                    },
                    "last_name": {
                        "type": "string",
                        "example": "Admin"
                    },
                    "image": {
                        "type": "string",
                        "example": "feyyaz"
                    },
                    "email": {
                        "type": "string",
                        "example": "dev@feyyaz.com.tr"
                    },
                    "phone": {
                        "type": "string",
                        "example": "905429322605"
                    },
                    "phone_code": {
                        "type": "string",
                        "example": "+90"
                    },
                    "account_status": {
                        "type": "boolean",
                        "example": true
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time"
                    }
                },
                "type": "object"
            }
        },
        "parameters": {
            "skip": {
                "name": "skip",
                "in": "query",
                "required": false,
